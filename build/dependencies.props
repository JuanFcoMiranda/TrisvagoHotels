    </PropertyGroup>

    <PropertyGroup Label="Swagger">
        <SwashbuckleAspNetCoreVersion>9.0.4</SwashbuckleAspNetCoreVersion>
    </PropertyGroup>

    <PropertyGroup Label="Mediatr">
        <MediatRVersion>12.5.0</MediatRVersion>
